<button id='go-time' >GO</button>
<script>

  const goBtn = document.getElementById('go-time')
  goBtn.addEventListener('click', () => {
    const win = window.open('/news', '_blank', "toolbar=yes,menubar=yes,location=yes,height=700,width=700")
    win.moveTo(1000, 1000)
    win.resizeTo(50, 50)
  })

</script>
